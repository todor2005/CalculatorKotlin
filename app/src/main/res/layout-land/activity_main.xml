<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:paddingLeft="10dp"
              android:paddingRight="10dp"
              android:paddingTop="10dp"
              android:paddingBottom="10dp"
              android:orientation="vertical"
              android:background="#000000"
              tools:context=".MainActivity">

    <TextView
            android:id="@+id/txtArea"
            android:layout_width="fill_parent"
            android:layout_height="90dp"
            android:text="@string/first_text"
            android:gravity="right"
            android:textSize="30dp"
            android:textColor="#FFFFFF"
            android:maxLines="2"
            android:maxLength="65"
            />

    <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="horizontal"
            >

        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical"
                android:layout_weight="1"
                android:paddingRight="3dp">

            <Button
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:text="@string/btnProcent"
                    android:id="@+id/btnProcent"
                    android:layout_weight="1"
                    android:textSize="22sp"
                    android:textStyle="bold"
                    android:background="@drawable/login_selector"
                    android:layout_marginBottom="3dp"
                    android:onClick="btnProcent"
                    />

            <Button
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:text="@string/btnSin"
                    android:id="@+id/btnSin"
                    android:layout_weight="1"
                    android:textSize="22dp"
                    android:textStyle="bold"
                    android:background="@drawable/login_selector"
                    android:layout_marginBottom="3dp"
                    android:onClick="btnSin"
                    />

            <Button
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:text="@string/btnLn"
                    android:id="@+id/btnLn"
                    android:layout_weight="1"
                    android:textSize="22dp"
                    android:textStyle="bold"
                    android:background="@drawable/login_selector"
                    android:layout_marginBottom="3dp"
                    android:onClick="btnLn"
                    />

            <Button
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:text="@string/btnExp"
                    android:id="@+id/btnExp"
                    android:layout_weight="1"
                    android:textSize="22dp"
                    android:textStyle="bold"
                    android:background="@drawable/login_selector"
                    android:layout_marginBottom="3dp"
                    android:onClick="btnExp"
                    />

        </LinearLayout>

        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical"
                android:layout_weight="1"
                android:paddingRight="3dp">

            <Button
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:text="@string/btnKoren"
                    android:id="@+id/btnKoren"
                    android:layout_weight="1"
                    android:textSize="22sp"
                    android:textStyle="bold"
                    android:background="@drawable/login_selector"
                    android:layout_marginBottom="3dp"
                    android:onClick="btnSQRT"
                    />

            <Button
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:text="@string/btnCos"
                    android:id="@+id/btnCos"
                    android:layout_weight="1"
                    android:textSize="22dp"
                    android:textStyle="bold"
                    android:background="@drawable/login_selector"
                    android:layout_marginBottom="3dp"
                    android:onClick="btnCos"
                    />

            <Button
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:text="@string/btnLog"
                    android:id="@+id/btnLog"
                    android:layout_weight="1"
                    android:textSize="22dp"
                    android:textStyle="bold"
                    android:background="@drawable/login_selector"
                    android:layout_marginBottom="3dp"
                    android:onClick="btnLog"
                    />

            <Button
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:text="@string/btnPi"
                    android:id="@+id/btnPi"
                    android:layout_weight="1"
                    android:textSize="22dp"
                    android:textStyle="bold"
                    android:background="@drawable/login_selector"
                    android:layout_marginBottom="3dp"
                    android:onClick="btnPi"
                    />

        </LinearLayout>

        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical"
                android:layout_weight="1"
                android:paddingRight="3dp">

            <Button
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:text="@string/btnStepen"
                    android:id="@+id/btnStepen"
                    android:layout_weight="1"
                    android:textSize="22sp"
                    android:textStyle="bold"
                    android:background="@drawable/login_selector"
                    android:layout_marginBottom="3dp"
                    android:onClick="btnStepen"
                    />

            <Button
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
